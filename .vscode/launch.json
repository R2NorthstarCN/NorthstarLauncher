{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "D:\\SteamLibrary\\steamapps\\common\\Titanfall2\\NorthstarLauncher.exe",
            "args": ["-dedicated"],
            "stopAtEntry": false,
            "cwd": "D:\\SteamLibrary\\steamapps\\common\\Titanfall2\\",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\msys64\\usr\\bin\\gdb.exe",
